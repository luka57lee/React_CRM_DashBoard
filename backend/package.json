{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "server": "node ./dist-server/bin/www",
    "dev": "cross-env NODE_ENV=development npm-run-all build server",
    "prod": "cross-env NODE_ENV=production npm-run-all build server",
    "transpile": "babel ./server --out-dir dist-server",
    "clean": "rimraf dist-server",
    "watch:dev": "nodemon",
    "test": "jest"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@signalwire/node": "^2.3.2",
    "babel-plugin-dotenv-import": "^2.0.0",
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "csv": "^5.3.1",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-status-monitor": "^1.2.8",
    "express-validator": "^6.4.0",
    "helmet": "^3.21.2",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "neat-csv": "^5.2.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "phone": "^2.4.7",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.3",
    "terminal-link": "^2.1.1",
    "winston": "^3.2.1"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      [
        "@babel/plugin-transform-runtime",
        {
          "regenerator": true
        }
      ],
      [
        "dotenv-import",
        {
          "moduleName": "@env",
          "path": ".env",
          "blacklist": null,
          "whitelist": null,
          "safe": false,
          "allowUndefined": false
        }
      ]
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0"
  }
}
